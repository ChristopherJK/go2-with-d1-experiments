			</geometry>
		</visual>	
	</link>
    <joint name="joint_3" type="revolute">
      <parent link="base_arm2"/>
      <child link="arm2"/>
      <origin xyz="0 0 0.043" rpy="0 0 0" />
      <axis xyz="0 1 0"/>
    <limit lower="-2.356" upper="2.356" />
    </joint>
    
    <link name="tilt">
		<visual>
          <origin xyz="0 0.03825 0" rpy="1.57 0 0" />
