            	<cylinder length="0.270" radius="0.026"/>
			</geometry>
		</visual>	
	</link>
    <joint name="joint_1" type="revolute">
      <parent link="base_rot"/>
      <child link="arm1"/>
      <origin xyz="0 0 0.02765" rpy="0 0 0" />
      <axis xyz="-1 0 0"/>
      <limit lower="-1.57" upper="1.57" />
    </joint>
        <link name="base_arm2">
		<visual>
          	<geometry>
                <box size="0.052 0.042 0.026" />
			</geometry>
		</visual>	
	</link>
    <joint name="joint_2" type="revolute">
