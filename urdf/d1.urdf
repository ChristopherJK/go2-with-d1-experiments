<robot name="robot_arm">
 	<link name="base_link">
		<visual>
		    <origin xyz="0 0 0.03725" rpy="0 0 0"/>
			<geometry>
				<cylinder length="0.0755" radius="0.065"/>
			</geometry>
		</visual>	
	</link>

    <link name="base_rot">
		<visual>
          	<geometry>
                <box size="0.052 0.042 0.0553" />
			</geometry>
		</visual>	
	</link>
    
    <joint name="joint_0" type="revolute">
      <parent link="base_link"/>
      <child link="base_rot"/>
      <origin xyz="0 0 0.10315" rpy="0 0 0" />
      <axis xyz="0 0 -1"/>
      <limit lower="-2.356" upper="2.356" />
    </joint>
    
    <link name="arm1">
		<visual>
          <origin xyz="0 0 0.135" rpy="0 0 0" />
